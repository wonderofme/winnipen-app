{
  "expo": {
    "name": "Winnipen",
    "slug": "winnipen",
    "version": "1.0.0",
    "owner": "wonderofme",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F1E8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.winnipen.app",
      "buildNumber": "10",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#243959"
      },
      "package": "com.winnipen.app",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow Winnipen to use your location to show posts near you."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Winnipen to access your photos to share images."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Winnipen to access your camera to take photos."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#243959",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "429ebc3d-96e4-46a0-b134-63d2d6914d98"
      },
      "apiUrl": "YOUR_API_URL_HERE",
      "firebaseApiKey": "YOUR_FIREBASE_API_KEY",
      "firebaseAuthDomain": "your-project.firebaseapp.com",
      "firebaseProjectId": "your-firebase-project-id",
      "firebaseStorageBucket": "your-project.firebasestorage.app",
      "firebaseMessagingSenderId": "YOUR_FIREBASE_SENDER_ID",
      "firebaseAppId": "YOUR_FIREBASE_APP_ID",
      "cloudinaryCloudName": "your-cloudinary-cloud-name",
      "cloudinaryApiKey": "your-cloudinary-api-key",
      "cloudinaryApiSecret": "your-cloudinary-api-secret",
      "privacyPolicyUrl": "https://wonderofme.github.io/winnipen-app/privacy-policy.html",
      "communityGuidelinesUrl": "https://wonderofme.github.io/winnipen-app/community-guidelines.html"
    }
  }
}
